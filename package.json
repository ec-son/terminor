{
  "name": "terminor",
  "version": "1.0.0",
  "description": "A CLI to translate between languages in the terminal",
  "author": {
    "name": "Amissi Ec-son",
    "email": "useniecson@gmail.com"
  },
  "keywords": [
    "ec",
    "cli",
    "terminor",
    "terminal",
    "console",
    "command"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "cli": "tsnd  --respawn dev/index.ts",
    "dev": "tsnd  --respawn src/test/main.ts",
    "dev1": "tsc && concurrently -k -n \"Typescript,Node\" -p \"[{name}]\" -c \"blue,green\" \"tsc --watch\" \"nodemon dist/test/main.js\"",
    "tsc": "tsc --watch",
    "node": "node dist/test/main.js",
    "test": "jest --watch"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "figlet": "^1.5.2",
    "inquirer": "^9.1.4",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.5",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@types/yargs": "^17.0.22",
    "concurrently": "^7.6.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
